{
    "name": "philly-theater-database",
    "version": "1.0.0",
    "description": "Automatically crawls the websites of Philly-area theaters to corral data about production history, award history, and demographic information over time",
    "main": "index.js",
    "author": "Joshua McLucas",
    "license": "ISC",
    "devDependencies": {
        "@graphql-codegen/cli": "^1.11.2",
        "@graphql-codegen/introspection": "^1.11.2",
        "@graphql-codegen/typescript": "^1.11.2",
        "@graphql-codegen/typescript-operations": "^1.11.2",
        "@graphql-codegen/typescript-react-apollo": "^1.11.2",
        "@graphql-codegen/typescript-resolvers": "^1.11.2",
        "graphql": "^14.5.8",
        "npm-watch": "^0.6.0"
    },
    "scripts": {
        "generate-types": "graphql-codegen --config codegen.yml",
        "watch": "npm-watch generate-types"
    },
    "watch": {
        "generate-types": "./server/schema.ts"
    },
    "repository": {
